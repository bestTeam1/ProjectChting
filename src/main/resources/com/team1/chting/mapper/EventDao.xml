<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team1.chting.dao.EventDao">
    <select id="enabledEvent" resultType="com.team1.chting.dto.EventDto">
        select event_no, event_img
        from event
        where enabled = 1;
    </select>
</mapper>